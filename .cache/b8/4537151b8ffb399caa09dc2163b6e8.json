/Block.js","original":{"line":49,"column":12}},{"generated":{"line":102,"column":21},"source":"components/Block.js","original":{"line":46,"column":8}},{"generated":{"line":102,"column":22},"source":"components/Block.js","original":{"line":45,"column":8}},{"generated":{"line":103,"column":0},"source":"components/Block.js","original":{"line":58,"column":5}},{"name":"Component","generated":{"line":107,"column":2},"source":"components/Block.js","original":{"line":6,"column":20}},{"generated":{"line":109,"column":0},"source":"components/Block.js","original":{"line":76,"column":1}},{"name":"Block","generated":{"line":110,"column":15},"source":"components/Block.js","original":{"line":78,"column":15}}],"sources":{"components/Block.js":"import React, { Component } from 'react';\nimport  { Button } from 'react-bootstrap'; \nimport Transaction from './Transaction'; \n\n\nclass Block extends Component {\n    state = { displayTransaction: false }; \n\n    toggleTransaction = () => {\n        this.setState({ displayTransaction : !this.state.displayTransaction })\n    }\n\n    get displayTransaction() {\n        const { data } = this.props.block;\n\n        const stringifiedData = JSON.stringify(data); \n\n        const dataDisplay = stringifiedData.length > 35 ? \n        `${stringifiedData.substring(0,35)}...` : \n        stringifiedData;\n\n        if (this.state.displayTransaction) {\n            return (\n            <div>\n                {\n                    data.map(transaction => (\n                        <div key={transaction.id}>\n                            <hr />\n                            <Transaction transaction={transaction} />\n                        </div>\n                    ))\n                }\n                <br />\n                <Button \n                    variant=\"outline-danger\"\n                    size=\"sm\"\n                    onClick={this.toggleTransaction}\n                >\n                    Show Less\n                </Button>\n            </div>\n            ); \n        }\n\n        return (\n        <div>\n            <div>Data: {dataDisplay}</div>\n            <br />\n            <Button \n                variant=\"outline-danger\"\n                size=\"sm\"\n                onClick={this.toggleTransaction}\n            >\n                Show More\n            </Button>\n        </div>\n        ); \n    }\n    render() {\n        console.log('this.displayTransaction', this.displayTransaction);\n       \n        const { timestamp, hash } = this.props.block;\n\n        const hashDisplay = `${hash.substring(0, 15)}...`;\n\n\n        return(\n            <div className='Block'>\n                <div>Hash: {hashDisplay}</div>\n                <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\n                {this.displayTransaction}\n            </div>\n        )\n         \n    }\n};\n\nexport default Block; "},"lineCount":null}},"error":null,"hash":"93e4b01342f6b0d377212d02e36b7149","cacheData":{"env":{}}}